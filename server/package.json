{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit false --outDir dist",
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "lint": "TIMING=1 eslint .",
    "postinstall": "prisma generate",
