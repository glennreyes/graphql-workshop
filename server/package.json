  },
  "dependencies": {
    "@graphql-yoga/render-graphiql": "^4.0.4",
    "@pothos/core": "^3.38.0",
    "@pothos/plugin-prisma": "^3.60.0",
    "@prisma/client": "^6.16.3",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.2",
    "graphql-yoga": "^4.0.4",
    "prisma": "^6.16.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@tsconfig/recommended": "^1.0.10",
    "@types/node": "^22.13.4",
    "eslint": "^9.36.0",
    "eslint-config-banana": "^1.7.3",
    "tsx": "^4.20.6"
  }
}
